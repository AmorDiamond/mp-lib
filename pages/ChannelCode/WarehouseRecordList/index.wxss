.record-list-page {
  display: flex;
  flex-direction: column;
  height: 100vh;
}
.warehouse-list-content {
  flex: 1;
  overflow: hidden;
}
.scroll-wrap {
  height: 100%;
}
.search-bar{
  width: 100%;
  height: 64px;
  background-color: white;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #DDDDDD;
  justify-content: space-around;
  box-sizing: border-box;
  padding: 0 32rpx;
}
.filter-icon {
  width: 24px;
  height: 24px;
  margin-left: 32rpx;
}
.search-wrap{
  flex: 1;
  height: 40px;
  display: flex;
  align-items: center;
  border: 1px solid #DDDDDD;
  border-radius: 2px;
}
.sreachimg {
  width: 16px;
  height: 16px;
  padding: 10px;
}
.sreachinput{
  flex: 1;
}
.palceColol{
  font-size:14px;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(146,146,146,1);
}
