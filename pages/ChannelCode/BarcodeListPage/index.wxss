/* pages/CodeWareHouse/CodeWareHouse.wxss */
.page-hd {
  height: 50px;
  margin-bottom: 12px;
}
.total-count {
  font-size: 15px;
  display: flex;
  padding: 0 32rpx 24rpx;
}
.total-count .count{
  font-weight: bold;
}
.total-count .num{
  color: #fa6400;
  margin: 0 4px;
}
.total-count .num:first-child{
  margin-left: 0;
}

.footer{
  width:100%;
  height:60px;
  background:rgba(255,255,255,1);
  box-shadow:0px -1px 10px 0px rgba(0,0,0,0.1);
  position: fixed;
  bottom: 0;
  display: flex;
  justify-content: space-around;
  align-items: center;
  padding: 0 16px;
  box-sizing: border-box;
  left: 0;
}
.colortwo{
  color:rgba(74,74,74,1);
}
.opt-btn {
  width:160px;
  line-height: 43px;
  padding: 0;
}

.warehouse-confirm-page {
  padding: 0 32rpx 60px;
}
.warehouse-confirm-page .total-count {
  padding: 0;
  margin: 32rpx 0;
}
.warehouse-confirm-page .footer button {
  width: 606rpx;
}

.dialog-content {
  padding: 40rpx;
  font-size: 14px;
  word-break: break-all;
}
.dialog-content .num {
  color: #FF6600;
}